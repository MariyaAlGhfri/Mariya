# encrypt quation:

massage=("|rx#duh#juhdwh")
         
            
for i in (massage):
    i=chr(ord(i)-3)
    print(i, end="")
    
    
 #perfect number
 
 n = int(input("enter number: "))
sum1 = 0
for i in range(1, n):
    if(n % i == 0):
        sum1 = sum1 + i
if (sum1 == n):
    print(" Perfect number")
else:
    print("not a Perfect number")
  
